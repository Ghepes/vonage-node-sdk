{
  "$schema": "https://typedoc-plugin-markdown.org/schema.json",
  "includeVersion": false,
  "entryPointStrategy": "packages",
  "outputFileStrategy": "modules",
  "flattenOutputFiles": true,
  "mergeReadme": true,
  "entryPoints": [
    "packages/*"
  ],
  "json": "docs/docs.json",
  "out": "docs",
  "logLevel": "Verbose",
  "plugin": [
    "typedoc-plugin-markdown"
  ]
}
